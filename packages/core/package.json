{
  "name": "lotus-plus",
  "version": "0.0.5",
  "description": "Components library by Vue3 + TS",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/index.css"
  ],
  "main": "./dist/umd/lotus-plus.umd.cjs",
  "module": "./dist/es/lotus-plus.js",
  "types": "./dist/types/core/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/lotus-plus.js",
      "require": "./dist/umd/lotus-plus.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "build": "run-p build-es build-umd",
    "build:watch": "run-p build-es:watch build-umd:watch",
    "build-es": "vite build --config vite.es.config.ts",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es:watch": "vite build --watch --config vite.es.config.ts",
    "build-umd:watch": "vite build --watch --config vite.umd.config.ts",
    "release": "release-it"
  },
  "devDependencies": {
    "vite-plugin-compression2": "^1.1.1",
    "vite-plugin-dts": "^3.9.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "peerDependencies": {
    "vue": "^3.4.19"
  },
  "keywords": [
    "vue3",
    "Components",
    "library",
    "TS",
    "Vue3",
    "Lotus-Plus"
  ],
  "author": "Rooon-K",
  "license": "ISC",
  "readme": "README.md"
}
